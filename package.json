    "dev": "nodemon -x 'npm run unit-test'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-nuget-plugin.git"
  },
  "keywords": [
