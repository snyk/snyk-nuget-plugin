  },
  "homepage": "https://github.com/snyk/snyk-nuget-plugin#readme",
  "dependencies": {
    "es6-promise": "^4.1.1",
    "xml2js": "^0.4.17",
    "zip": "^1.2.0"
  },
  "devDependencies": {
    "jscs": "^3.0.7",
    "nodemon": "^1.12.1",
    "semantic-release": "^8.2.0",
    "tap": "^10.7.0",
    "tap-only": "0.0.5"
  }
